#include<iostream>
#include<stack>
#include<queue>
using namespace std;
int main() {
	stack<int> S;
	stack<int> Q;
	int x;
	while (cin >> x && x != 0) {
		if (x > 0)
			S.push(x);
		else
			Q.push(x);
	}
	while (!S.empty())
	{
		x = S.top();
		S.pop();
		cout << x << "\t";
	}
	cout << endl;
	while (!Q.empty())
	{
		x = Q.top();
		Q.pop();
		cout << x << "\t";
	}
	cout << endl;
}
